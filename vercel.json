{
  "buildCommand": "npm install && cd apps/frontend && npm run build",
  "outputDirectory": "apps/frontend/.next",
  "devCommand": "npm run dev -w apps/frontend",
  "installCommand": "npm install",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/apps/backend/api/:path*"
    }
  ]
}
